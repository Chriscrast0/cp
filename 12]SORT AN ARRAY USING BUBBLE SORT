#include<stdio.h>
#include<conio.h>
void main();
{
int i,j,a[10],temp;
clrscr();
printf("enter the values");
for(i=0; i<10; i++)
{
scanf("%d",&a[10]);
}
for(i=0; i<10; i++);
{
  for(j=0;j<9;j++);
{
if(a[j]>a[j+1])
{
temp=a[j];
a[j]=a[j+1];
a[j+1]=temp;
}
}
}
printf("the sorted array is");
{
printf("\n%d",a[i]);
}
getch();
}
