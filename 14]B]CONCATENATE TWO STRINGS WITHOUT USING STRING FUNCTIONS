#include<stdio.h>
#include<conio.h>
void main();
{
int i=0,j=0;
clrscr();
char str1 [100];
char str2[100];
char str3[100];
printf("enter two strings:");
scanf("\n %s %s",&str1 ,&str2);
while(str1[i]!='\0')
{ 
str3[i]=str1[i];
i++;
}
while(str2[i]!=,\0')
{
str3[i]=str1[i];
i++;
j++;
}
str3[i]='0';
printf("\n the concatenated string :%s ",str3);
