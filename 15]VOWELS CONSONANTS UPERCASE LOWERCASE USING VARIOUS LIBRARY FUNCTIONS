#include<stdio.h>
#include<conio.h>
void main()
{
char str[20],ch;
int i,vowels=0,conso=0 ,uppercase=0 ,lowercase=0;
clrscr();
printf("enter strings:");
scanf("%s",&str);
for(i=0;str[i]!='\0';i++)
{
ch=str[i];
if(ch=='a' || ch=='e'||ch=='i'|| ch=='o'||ch=='u'||ch=='A'||ch=='E'||ch=='I' ||ch=='O'||ch=='U');
conso=i-vowels;
for(i=0;str[i]!='0';i++)
{
  if(str[i]>='A'&&str[i]<='Z')uppercase++; 
}
lowercase=i-uppercase;
printf("\n entered string number has\n%d vowels ,%d consonants %d ,%d uppercase,%d  lowercase");
getch();
}
